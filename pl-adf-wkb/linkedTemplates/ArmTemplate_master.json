{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "pl-adf-wkb"
		},
		"AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase'"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorageLinkedService'"
		},
		"LS_AzureSynapseArtifacts_WKB_properties_typeProperties_endpoint": {
			"type": "string",
			"defaultValue": "https://pl-synapse-wb.dev.azuresynapse.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "pl-adf-wkb_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase_connectionString": {
						"value": "[parameters('AzureSqlDatabase_connectionString')]"
					},
					"AzureStorageLinkedService_connectionString": {
						"value": "[parameters('AzureStorageLinkedService_connectionString')]"
					},
					"LS_AzureSynapseArtifacts_WKB_properties_typeProperties_endpoint": {
						"value": "[parameters('LS_AzureSynapseArtifacts_WKB_properties_typeProperties_endpoint')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}